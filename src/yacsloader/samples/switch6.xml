<proc>
    <!--
    -->
    <!--types -->
    <!--nodes -->
    <inline name="n" >
        <script>
            <code>select=3</code>
        </script>
        <outport name="select" type="int"/>
    </inline>
    <inline name="n2" >
        <script>
            <code>print p1</code>
        </script>
        <inport name="p1" type="double"/>
        <outport name="p1" type="double"/>
    </inline>
    <bloc name="b">
    <switch name="b1">
      <case id="1">
        <inline name="n2" >
          <script><code>print p1</code></script>
          <inport name="p1" type="double"/>
          <outport name="p1" type="double"/>
        </inline>
      </case>
      <case id="3">
          <inline name="n2" >
            <script><code>print p1</code></script>
            <inport name="p1" type="double"/>
            <outport name="p1" type="double"/>
          </inline>
      </case>
      <default>
        <inline name="n2" >
          <script><code>print p1</code></script>
          <inport name="p1" type="double"/>
          <outport name="p1" type="double"/>
        </inline>
      </default>
    </switch>
    </bloc>
    <!--control links -->
    <control> <fromnode>n</fromnode> <tonode>b</tonode> </control>
    <control> <fromnode>b</fromnode> <tonode>n2</tonode> </control>
    <!--data links -->
    <datalink> <fromnode>n</fromnode><fromport>select</fromport>
               <tonode>b.b1</tonode> <toport>select</toport> </datalink>
    <datalink> <fromnode>b.b1.p1_n2</fromnode><fromport>p1</fromport>
               <tonode>n2</tonode> <toport>p1</toport> </datalink>
    <datalink> <fromnode>b.b1.p3_n2</fromnode><fromport>p1</fromport>
               <tonode>n2</tonode> <toport>p1</toport> </datalink>
    <datalink> <fromnode>b.b1.default_n2</fromnode><fromport>p1</fromport>
               <tonode>n2</tonode> <toport>p1</toport> </datalink>
    <!--parameters -->
    <parameter>
        <tonode>b.b1.p1_n2</tonode> <toport>p1</toport>
        <value><double>23</double> </value>
    </parameter>
    <parameter>
        <tonode>b.b1.p3_n2</tonode> <toport>p1</toport>
        <value><double>54</double> </value>
    </parameter>
    <parameter>
        <tonode>b.b1.default_n2</tonode> <toport>p1</toport>
        <value><double>67</double> </value>
    </parameter>

</proc>
